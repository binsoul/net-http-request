{
    "name": "binsoul/net-http-request",
    "description": "Extended ServerRequest class and request factory",
    "keywords": [
        "http",
        "request"
    ],
    "homepage": "https://github.com/binsoul/net-http-request",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Mößler",
            "email": "code@binsoul.de",
            "homepage": "https://github.com/binsoul",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "binsoul/net": "dev-master",
        "binsoul/net-http-message-bridge": "dev-master",
        "binsoul/net-http-message-message" : "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "fabpot/php-cs-fixer": "^1.0"
    },
    "suggest": {
        "mobiledetect/mobiledetectlib": "For better device type detection (including tablets)"
    },
    "autoload": {
        "psr-4": {
            "BinSoul\\Net\\Http\\Request\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BinSoul\\Test\\Net\\Http\\Request\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix-src": "php-cs-fixer fix -v src",
        "fix-tests": "php-cs-fixer fix -v tests"
    }
}
