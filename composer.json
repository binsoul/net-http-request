{
    "name": "binsoul/net-http-request",
    "description": "Extended ServerRequest class and request factory",
    "keywords": [
        "network",
        "http",
        "request"
    ],
    "homepage": "https://github.com/binsoul/net-http-request",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Mößler",
            "email": "code@binsoul.de",
            "homepage": "https://github.com/binsoul",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "binsoul/common": "~1.0@dev",
        "binsoul/io-stream": "~1.0@dev",
        "binsoul/net": "~1.0@dev",
        "binsoul/net-http-message-bridge": "~1.0@dev",
        "binsoul/net-http-message-message": "~1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "fabpot/php-cs-fixer": "^1.0"
    },
    "suggest": {
        "mobiledetect/mobiledetectlib": "For better device type detection (including tablets)"
    },
    "autoload": {
        "psr-4": {
            "BinSoul\\Net\\Http\\Request\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BinSoul\\Test\\Net\\Http\\Request\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": [
            "php-cs-fixer fix src",
            "php-cs-fixer fix tests"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
